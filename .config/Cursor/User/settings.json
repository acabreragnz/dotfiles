{
  "editor.fontFamily": "'Cascadia Code', 'Cascadia Code Italic','Droid Sans Mono', 'monospace', monospace",
  "editor.fontSize": 15,
  "editor.fontLigatures": true,
  "editor.lineHeight": 1.75,
  "editor.minimap.enabled": true,
  "editor.rulers": [
    120
  ],
  "editor.stickyScroll.enabled": true,
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.mouseWheelZoom": true,
  "editor.wordWrap": "on",
  "editor.wordSeparators": "`~!@#%^&*()=+[{]}\\|;:'\",.<>/?",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.tabSize": 2,
  "editor.insertSpaces": false,
  "editor.detectIndentation": true,
  "editor.codeActionsOnSave": {
    "source.sortImports": "always",
    "source.addMissingImports.ts": "always"
  },
  "editor.codeActions.triggerOnFocusChange": true,
  "editor.inlineSuggest.enabled": true,
  "editor.linkedEditing": true,
  "editor.lightbulb.enabled": "true",
  "editor.quickSuggestions": {
    "strings": true
  },
  "editor.smartSelect.selectSubwords": false,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.guides.bracketPairs": true,
  "editor.guides.bracketPairsHorizontal": true,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "Vue.volar"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.preferences.quoteStyle": "single",
  "typescript.preferences.importModuleSpecifierEnding": "minimal",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "javascript.preferences.importModuleSpecifierEnding": "minimal",
  "javascript.inlayHints.parameterNames.suppressWhenArgumentMatchesName": false,
  "javascript.inlayHints.variableTypes.suppressWhenTypeMatchesName": false,
  "template-string-converter.autoRemoveTemplateString": true,
  "prettier.singleQuote": true,
  "prettier.printWidth": 120,
  "prettier.useTabs": true,
  "vue.codeActions.savingTimeLimit": 3000,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "vue-html": "html"
  },
  "vite.autoStart": false,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.ignoreRebaseWarning": true,
  "scm.diffDecorationsGutterWidth": 5,
  "scm.workingSets.enabled": false,
  "gitlens.ai.model": "vscode",
  "gitlens.ai.vscode.model": "copilot:gpt-4.1",
  "gitlens.changes.locations": [
    "gutter",
    "line",
    "overview"
  ],
  "gitlens.currentLine.uncommittedChangesFormat": "\u270f\ufe0f ${ago}",
  "gitlens.heatmap.locations": [
    "gutter",
    "overview"
  ],
  "gitlens.views.commitDetails.files.layout": "list",
  "githubPullRequests.createOnPublishBranch": "never",
  "githubPullRequests.pullBranch": "never",
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": false,
    "scminput": false,
    "typescriptreact": false
  },
  "github.copilot.editor.enableAutoCompletions": true,
  "github.copilot.nextEditSuggestions.enabled": true,
  "cursor.cpp.disabledLanguages": [
    "markdown",
    "scminput",
    "typescriptreact"
  ],
  "terminal.external.linuxExec": "warp",
  "terminal.external.osxExec": "Warp.app",
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "terminal.integrated.env.linux": {},
  "workbench.colorTheme": "One Dark Pro Darker",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.activityBar.orientation": "vertical",
  "workbench.colorCustomizations": {
    "editorIndentGuide.activeBackground": "#f7c111"
  },
  "workbench.editor.closeEmptyGroups": true,
  "workbench.editor.enablePreviewFromCodeNavigation": true,
  "workbench.editor.enablePreviewFromQuickOpen": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.pinnedTabsOnSeparateRow": true,
  "workbench.editor.wrapTabs": true,
  "workbench.editor.customLabels.enabled": true,
  "workbench.editor.customLabels.patterns": {
    "**/pages/**/*.vue": "${filename} - Page \ud83d\udcc4",
    "**/components/**/*.vue": " ${filename} - Component \ud83e\udde9",
    "**/pages/**/components/**/*.vue": "${filename} - Component \ud83e\udde9",
    "**/composables/**/*": "${filename} - Composable \u2699\ufe0f",
    "**/composables/**/*.{test,spec}.*": "${filename} - Test \ud83e\uddea",
    "**/mixins/**/*": "${filename} - Mixin",
    "**/store*/**/*": "${filename} - Store \ud83e\udde0",
    "**/api/**/*": "${filename} - API \ud83c\udf10",
    "**/*.{test,spec}.*": "${filename} - Test \ud83e\uddea"
  },
  "workbench.editorAssociations": {
    "git-rebase-todo": "default"
  },
  "workbench.externalBrowser": "chrome",
  "workbench.settings.applyToAllProfiles": [
    "typescript.referencesCodeLens.enabled",
    "typescript.referencesCodeLens.showOnAllFunctions",
    "typescript.implementationsCodeLens.showOnInterfaceMethods",
    "typescript.implementationsCodeLens.enabled",
    "editor.snippetSuggestions",
    "emmet.includeLanguages",
    "typescript.inlayHints.parameterTypes.enabled"
  ],
  "files.autoSave": "onFocusChange",
  "files.associations": {
    "*.config": "yaml"
  },
  "files.watcherExclude": {
    "**/.output/**": true,
    "**/.yarn/**": true,
    "**/node_modules/**": true,
    "**/vendor/**": true
  },
  "search.exclude": {
    "**/dist": true,
    "**/.fvm": true
  },
  "diffEditor.codeLens": true,
  "diffEditor.ignoreTrimWhitespace": true,
  "security.promptForLocalFileProtocolHandling": false,
  "security.workspace.trust.untrustedFiles": "open",
  "debug.showVariableTypes": true,
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#FF2D00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "now",
      "color": "#00D084",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],
  "todo-tree.general.tags": [
    "XXX",
    "TODO",
    "NOW",
    "HACK",
    "FIXME",
    "BUG",
    "MIGRATE"
  ],
  "todo-tree.highlights.enabled": false,
  "console-ninja.featureSet": "Community",
  "browse-lite.chromeExecutable": "/usr/bin/google-chrome",
  "database-client.highlightSQLBlock": true,
  "database-client.showFilter": true,
  "cSpell.userWords": [
    "Interactor"
  ],
  "jest.autoRun": "off",
  "npm.keybindingsChangedWarningShown": true,
  "open-in-browser.default": "chrome",
  "explorer.confirmPasteNative": false,
  "window.customTitleBarVisibility": "auto",
  "figma.autocompleteBlocks": true,
  "docker.extension.enableComposeLanguageServer": false
}